{
  "name": "upsub",
  "description": "Command line interface for UpSub",
  "version": "0.2.2",
  "license": "MIT",
  "scripts": {
    "test": "npm run lint && jest --coverage",
    "test:watch": "jest --watch",
    "lint": "standard | snazzy; exit 0",
    "lint:fix": "standard --fix | snazzy; exit 0",
    "build": "babel src/ -D -d ./lib; exit 0",
    "build:watch": "babel src/ -D -d ./lib --watch",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run build && npm test"
  },
  "bin": {
    "upsub": "./lib/index.js"
  },
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "upsub",
    "cli",
    "message-bus",
    "realtime",
    "async",
    "request",
    "response",
    "websocket"
  ],
  "author": {
    "name": "Frederik Kvartborg Albertsen",
    "email": "hello@kvartborg.net",
    "url": "https://kvartborg.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/upsub/cli"
  },
  "bugs": {
    "url": "/issues"
  },
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "coveralls": "^2.13.1",
    "jest": "^21.1.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2"
  },
  "jest": {
    "modulePaths": [
      "src"
    ]
  },
  "dependencies": {
    "@upsub/client": "^0.2.0",
    "chalk": "^2.3.2",
    "cli-highlight": "^1.2.3",
    "meow": "^4.0.0"
  }
}
